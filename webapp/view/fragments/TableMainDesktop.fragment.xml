<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
    <Table items="{oModelProyect>/oReporte}"  mode="None" growingThreshold="20">
        <columns>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColOrder}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColPDF}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColCustomer}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColCompName}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColOrderType}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColDocReference}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColReference}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColCurrency}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColAmount}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColOrderDate}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColDateDispatch}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColState}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleModify}" width="auto" />
                </header>
            </Column>
        </columns>
       <items>
            <ColumnListItem type="Inactive">
                <cells>
                    <Button type="Transparent" text="{path:'oModelProyect>txt1'}" press="_onPressNavigateDetail"/>
                    <Button icon="sap-icon://pdf-attachment" />
                    <Text text="{path:'oModelProyect>txt2'}"/>
                    <Text text="{path:'oModelProyect>txt3'}"/>
                    <Text text="{path:'oModelProyect>txt4'}"/>
                    <Text text="{path:'oModelProyect>txt5'}"/>
                    <Text text="{path:'oModelProyect>txt6'}"/>
                    <Text text="{path:'oModelProyect>txt7'}"/>
                    <Text text="{path:'oModelProyect>txt8'}"/>
                    <Text text="{path:'oModelProyect>txt9'}"/>
                    <Text text="{path:'oModelProyect>txt10'}"/>
                    <Text text="{path:'oModelProyect>txt11'}"/>
                    <Button icon="sap-icon://circle-task" visible="{path:'oModelProyect>txt12'}"/>
                    
                </cells>
            </ColumnListItem>
        </items>
     
    </Table>
</core:FragmentDefinition>