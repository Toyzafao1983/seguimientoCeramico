<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
    <Table items="{oModelProyect>/oReporte}"  mode="None" growingThreshold="20">
        <headerToolbar>
            <Toolbar>
                <ToolbarSpacer/>
                <Button text="{i18n>textbtnPrueba}" press="_onPressPrueba" type="Critical" icon="sap-icon://pdf-attachment" tooltip="{i18n>textbtnDelivery}"/>
            </Toolbar>
        </headerToolbar>
        <columns>
            <Column width="6rem" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColNotificationDate}" width="auto" />
                </header>
            </Column>
            <!-- <Column width="5rem" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColNotificationHour}" width="auto" />
                </header>
            </Column> -->
            <Column width="7rem" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColTechnicalLocation}" width="auto" />
                </header>
            </Column>
            <Column width="auto" hAlign="Begin" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColDenTechnicalLocation}" width="auto" wrapping="true"/>
                </header>
            </Column>
            <Column width="6rem" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColNotification}" width="auto" wrapping="true"/>
                </header>
            </Column>
            <Column width="8rem" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColDescription}" width="auto" wrapping="true"/>
                </header>
            </Column>
            <Column width="7rem" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColState}" width="auto" wrapping="true"/>
                </header>
            </Column>
            <Column width="7rem" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColPlanningGroup}" width="auto" wrapping="true"/>
                </header>
            </Column>
            <Column width="6rem" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColCodeResponsible}" width="auto" wrapping="true"/>
                </header>
            </Column>
            <Column width="6rem" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColResponsible}" width="auto" wrapping="true"/>
                </header>
            </Column>
            <Column width="5rem" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColCodeBloq}" width="auto" wrapping="true"/>
                </header>
            </Column>
            <Column width="auto" hAlign="Center" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                <header>
                    <Text text="{i18n>titleColBloq}" width="auto" wrapping="true"/>
                </header>
            </Column>
        </columns>
        <items>
            <ColumnListItem type="Inactive">
                <cells>
                    <Text text="{path:'oModelProyect>Erdat', formatter:'.formatter.formatYYYYMMDDDateAbapDateHourSlash'}"/>
                    <!-- <Text text="{path:'oModelProyect>txt2', formatter:'.formatter.formatHHMMSSAbap'}"/> -->
                    <Text text="{path:'oModelProyect>Tplnr'}"/>
                    <Text text="{path:'oModelProyect>Pltxt'}"/>
                    <Text text="{path:'oModelProyect>Qmnum'}"/>
                    <Text text="{path:'oModelProyect>Qmtxt'}"/>
                    <Text text="{path:'oModelProyect>Txt30'}"/>
                    <Text text="{path:'oModelProyect>Innam'}"/>
                    <Text text="{path:'oModelProyect>Oteil'}"/>
                    <Text text="{path:'oModelProyect>Kurztex1'}"/>
                    <Text text="{path:'oModelProyect>Fecod'}"/>
                    <Text text="{path:'oModelProyect>Kurztex2'}"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</core:FragmentDefinition>